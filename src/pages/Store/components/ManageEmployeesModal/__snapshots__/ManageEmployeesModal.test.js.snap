            }
          />
          <EventStack
            name="mousedown"
            on={[Function]}
            pool="Modal"
            target="document"
          />
          <EventStack
            name="click"
            on={[Function]}
            pool="Modal"
            target="document"
          />
          <EventStack
            name="keydown"
            on={[Function]}
            pool="Modal"
            target="document"
          />
        </Portal>
      </Modal>
    </_default>
  </ManagementModalContainer>
</_default>
